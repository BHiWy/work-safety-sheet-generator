<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_1.xsd"
             version="3.1">
  <persistence-unit name="generator">
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/generator"/>
      <property name="javax.persistence.jdbc.user" value="gen_user"/>
      <property name="javax.persistence.jdbc.password" value="Generator123."/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
    </properties>
  </persistence-unit>
</persistence>
